    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "tslint-clean-code": "^0.2.9",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.15.0",
    "tslint-immutable": "^5.2.0",
    "tslint-microsoft-contrib": "^6.0.0",
