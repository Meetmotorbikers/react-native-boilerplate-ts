    "typescript-tslint-plugin": "^0.3.1"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios",
    "android": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "test": "jest",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "lint:css": "stylelint './src/**/*.tsx'",
    "xcode": "open ios/react_native_boilerplate_ts.xcodeproj",
    "storybook": "storybook start --config-dir './tools/storybook'",
    "prestorybook": "rnstl",
    "i18n-dts": "i18n-dts",
    "i18n-dts:watch": "i18n-dts -w"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/stories"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./tools/storybook/storyLoader.js"
    }
  },
  "i18n-dts": {
    "model": "./src/locale/languages/en.ts",
    "outputDir": "./types"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{tsx}": [
      "npm run lint:css",
      "git add"
    ],
    "*.{ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  }
}
