    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "husky": "^1.3.1",
    "i18n-dts": "^0.2.0",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "lint-staged": "^8.1.4",
    "metro-react-native-babel-preset": "0.52.0",
